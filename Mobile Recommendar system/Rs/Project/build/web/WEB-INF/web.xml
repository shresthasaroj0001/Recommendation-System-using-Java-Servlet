<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>userCOntroller</servlet-name>
        <servlet-class>com.controller.userLogin.LoginUserCOntroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logOutController</servlet-name>
        <servlet-class>com.controller.userLogin.logOutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>com.search.col.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>com.registerFone.coln.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploaaadController</servlet-name>
        <servlet-class>com.upload.controler.UploaaadController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpController</servlet-name>
        <servlet-class>com.controller.userLogin.SignUpController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testControler</servlet-name>
        <servlet-class>com.rating.coln.testControler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userCOntroller</servlet-name>
        <url-pattern>/userCOntroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logOutController</servlet-name>
        <url-pattern>/logOutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/SearchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/RegisterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploaaadController</servlet-name>
        <url-pattern>/UploaaadController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpController</servlet-name>
        <url-pattern>/SignUpController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testControler</servlet-name>
        <url-pattern>/testControler</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
